{
    "name": "personal-website",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "homepage": ".",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check",
        "format:fix": "prettier . --write",
        "lines": "cloc . --exclude-dir=dist,node_modules,.next --include-ext=ts,tsx --exclude-ext=d.ts,gen.ts",
        "lines:large": "cloc . --exclude-dir=dist,node_modules,.next --include-ext=ts,tsx --exclude-ext=d.ts,gen.ts --by-file --csv | awk -F, '$5 >= 500 {print $2, $5}'",
        "tree": "tree -I node_modules -I dist -I node_modules -I public"
    },
    "dependencies": {
        "@react-three/drei": "^9.119.0",
        "@react-three/fiber": "^8.17.10",
        "body-parser": "^1.20.3",
        "emailjs-com": "^3.2.0",
        "express": "^4.21.2",
        "framer-motion": "^12.0.6",
        "gsap": "^3.12.7",
        "nodemailer": "^7.0.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.5.0",
        "react-intersection-observer": "^9.14.1",
        "react-router-dom": "^6.29.0",
        "tailwind-scrollbar": "^3.1.0",
        "three": "^0.171.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "globals": "^17.0.0",
        "postcss": "^8.4.49",
        "prettier": "^3.7.4",
        "tailwindcss": "^3.4.15",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.53.0",
        "vite": "^7.3.1"
    }
}
